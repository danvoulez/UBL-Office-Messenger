{
  "share_target": {
    "action": "/#/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "files",
          "accept": [
            "image/*",
            "text/plain",
            "application/pdf"
          ]
        }
      ]
    }
  }
}